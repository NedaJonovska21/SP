#include <iostream>
using namespace std;

int main() {
     int m,n, matrix[50][50];
     cin>>n;
     m=n*2;
     
     for(int i=0; i<n; i++)
     {
         for(int j=0; j<m; j++)
         {
             cin>>matrix[i][j];
         }

     }

     for(int i=0; i<n; i++)
     {
         for(int j=0; j<m/2; j++)
         {
             cout<<matrix[i][j]<<" ";
         }
         cout<<endl;
     }

     for(int i=0; i<n; i++)
     {
         for(int j=m/2; j<m; j++)
         {
             cout<<matrix[i][j]<<" ";
         }
         cout<<endl;
     }

    return 0;
}
