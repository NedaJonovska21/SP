#include <iostream>
#include <cstring>
using namespace std;
bool ispalindrom(char *s, int i)
{
    if(i == strlen(s)/2)
    {
        return true;
    }
    if(*(s+i)==*(s+ strlen(s-i-1)))
    {
        return ispalindrom(s, i+1);
    }
    else
    {
        return false;
    }

}
int main() {
    char s[100];
    int i;
    cin.getline(s,100);

    bool result= ispalindrom(s,i);

    if(result)
    {
        cout<<"Nizata e palindrom"<<endl;
    }
    else
    {
        cout<<"Nizata NE e palindrom"<<endl;
    }

    return 0;
}
