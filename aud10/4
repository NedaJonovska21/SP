#include <iostream>
using namespace std;
int count_char(char s[], char c)
{
    int br=0;
    for(int i=0; s[i]!='\0'; i++)
    {
        if(s[i]==c)
        {
            br++;
        }
    }
    return br;
}

int main() {
    char s[100], c;
    cin.getline(s,100);

    cin>>c;

    cout << count_char(s,c);
    return 0;
}
