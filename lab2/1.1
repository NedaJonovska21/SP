#include <iostream>
using namespace std;

int main() {
    int n;
    int c1, c2, c3, c5;
    bool isTrue=false;

    cin>>n;
    if(n<10000 || n>99999)
    {
        cout<<"vneseniot broj e nevaliden"<<endl;

    }
    else
    {
       c1=n/10000;
       c2=n/1000%10;
       c3=n/100%10;
       c5=n%10;

        int zbir=c2+c3;
       if(n%zbir==0)
       {
           if(c1<c3 && c3<c5 && c1<c5)
           {
               isTrue=true;
           }
       }
       if(isTrue)
       {
           cout<<"1"<<endl;
       }
       else
       {
           cout<<"0"<<endl;
       }
    }


}
