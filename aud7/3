#include <iostream>
using namespace std;

int main() {
    int a[100], b[100], n, product=0;

    cin>>n;

    for(int i=0; i<n; i++)
    {
        cin>>a[i];
    }
    for(int i=0; i<n; i++)
    {
        cin>>b[i];
    }
    for(int i=0; i<n; i++)
    {
        product+=a[i]*b[i];
    }

    cout<<"The scalar product is: "<<product<<endl;
    return 0;
}
